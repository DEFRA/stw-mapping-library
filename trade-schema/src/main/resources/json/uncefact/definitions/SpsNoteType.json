{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "id": "SpsNoteType.json",
  "type": "object",
  "properties": {
    "content": {
      "type": "array",
      "items": {
        "allOf": [
          {
            "$ref": "TextType.json"
          },
          {
            "required": [
              "value"
            ]
          }
        ]
      }
    },
    "contentCode": {
      "type": "array",
      "items": {
        "$ref": "CodeType.json"
      }
    },
    "subject": {
      "$ref": "TextType.json"
    },
    "subjectCode": {
      "$ref": "CodeType.json"
    }
  },
  "required": [
    "content"
  ]
}
