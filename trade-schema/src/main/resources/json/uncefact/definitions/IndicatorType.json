{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "id": "IndicatorType.json",
  "IndicatorType": {
    "type": "object",
    "properties": {
      "indicator": {
        "type": "boolean"
      },
      "indicatorString": {
        "type": "object",
        "properties": {
          "format": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
      }
    },
    "oneOf": [
      {
        "required": [
          "indicator"
        ]
      },
      {
        "required": [
          "indicatorString"
        ]
      }
    ]
  }
}