{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "allOf": [
    {
      "$ref": "../uncefact/SPSCertificate.json"
    },
    {
      "properties": {
        "spsConsignment": {
          "properties": {
            "availabilityDueDateTime": {
              "required": [
                "dateTime"
              ]
            },
            "includedSpsConsignmentItem": {
              "items": {
                "properties": {
                  "includedSpsTradeLineItem": {
                    "items": {
                      "properties": {
                        "additionalInformationSpsNote": {
                          "items": {
                            "required": [
                              "subjectCode"
                            ]
                          }
                        },
                        "applicableSpsClassification": {
                          "$ref": "definitions/ApplicableSpsClassification.json"
                        },
                        "grossVolumeMeasure": {
                          "properties": {
                            "unitCode": {
                              "enum": [
                                "LTR",
                                "MTQ"
                              ]
                            }
                          },
                          "required": [
                            "value",
                            "unitCode"
                          ]
                        },
                        "grossWeightMeasure": {
                          "required": [
                            "value",
                            "unitCode"
                          ]
                        },
                        "netWeightMeasure": {
                          "required": [
                            "value",
                            "unitCode"
                          ]
                        },
                        "netVolumeMeasure": {
                          "properties": {
                            "unitCode": {
                              "const": "H87"
                            }
                          },
                          "required": [
                            "value",
                            "unitCode"
                          ]
                        },
                        "physicalSpsPackage": {
                          "items": {
                            "properties": {
                              "levelCode": {
                                "properties": {
                                  "value": {
                                    "const": "4"
                                  }
                                }
                              }
                            }
                          }
                        }
                      },
                      "required": [
                        "applicableSpsClassification",
                        "originSpsCountry",
                        "physicalSpsPackage",
                        "scientificName"
                      ]
                    }
                  }
                }
              }
            },
            "mainCarriageSpsTransportMovement": {
              "items": {
                "properties": {
                  "id": {
                    "properties": {
                      "schemeAgencyID": {
                        "const": "GB"
                      },
                      "schemeID": {
                        "enum": [
                          "ship_imo_number_before_bcp",
                          "train_identifier_before_bcp",
                          "road_vehicle_registration_before_bcp",
                          "airplane_flight_number_before_bcp",
                          "ship_imo_number_after_bcp",
                          "train_identifier_after_bcp",
                          "road_vehicle_registration_after_bcp",
                          "airplane_flight_number_after_bcp"
                        ]
                      }
                    },
                    "required": [
                      "schemeAgencyID",
                      "schemeID"
                    ]
                  },
                  "modeCode": {
                    "properties": {
                      "value": {
                        "enum": [
                          "0",
                          "1",
                          "2",
                          "3",
                          "4"
                        ]
                      }
                    }
                  }
                },
                "required": [
                  "id"
                ]
              }
            },
            "unloadingBaseportSpsLocation": {
              "properties": {
                "id": {
                  "properties": {
                    "schemeID": {
                      "enum": [
                        "un_locode",
                        "IPAFFS_POE"
                      ]
                    }
                  },
                  "required": [
                    "schemeID"
                  ]
                }
              },
              "required": [
                "id"
              ]
            },
            "utilizedSpsTransportEquipment": {
              "items": {
                "properties": {
                  "id": {
                    "properties": {
                      "value": {
                        "minLength": 1,
                        "maxLength": 32
                      },
                      "schemeID": {
                        "const": "container_number"
                      }
                    },
                    "required": [
                      "schemeID"
                    ]
                  },
                  "affixedSpsSeal": {
                    "minItems": 1,
                    "maxItems": 1,
                    "items": {
                      "properties": {
                        "id": {
                          "properties": {
                            "value": {
                              "minLength": 1,
                              "maxLength": 32
                            },
                            "schemeID": {
                              "enum": [
                                "seal_number",
                                "official_seal_number"
                              ]
                            }
                          },
                          "required": [
                            "schemeID"
                          ]
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "required": [
            "exportSpsCountry",
            "unloadingBaseportSpsLocation"
          ]
        },
        "spsExchangedDocument": {
          "properties": {
            "id": {
              "properties": {
                "value": {
                  "minLength": 1,
                  "maxLength": 90
                }
              }
            },
            "includedSpsNote": {
              "items": {
                "if": {
                  "properties": {
                    "subjectCode": {
                      "properties": {
                        "value": {
                          "const": "PRODUCT_TEMPERATURE"
                        }
                      },
                      "required": [
                        "value"
                      ]
                    }
                  },
                  "required": [
                    "subjectCode"
                  ]
                },
                "then": {
                  "properties": {
                    "contentCode": {
                      "items": {
                        "properties": {
                          "value": {
                            "enum": [
                              "AMBIENT",
                              "CHILLED",
                              "FROZEN"
                            ]
                          }
                        }
                      }
                    },
                    "content": {
                      "items": {
                        "properties": {
                          "value": {
                            "const": ""
                          }
                        }
                      }
                    }
                  },
                  "required": [
                    "contentCode"
                  ]
                },
                "properties": {
                  "contentCode": {
                    "minItems": 1
                  }
                },
                "required": [
                  "subjectCode"
                ]
              }
            },
            "signatorySpsAuthentication": {
              "items": {
                "properties": {
                  "includedSpsClause": {
                    "items": {
                      "properties": {
                        "id": {
                          "properties": {
                            "value": {
                              "enum": [
                                "PURPOSE",
                                "GOODS_CERTIFIED_AS",
                                "INTERNAL_MARKET_PURPOSE",
                                "ANIMALS_CERTIFIED_AS"
                              ]
                            }
                          }
                        }
                      },
                      "required": [
                        "id"
                      ]
                    }
                  }
                }
              }
            },
            "statusCode": {
              "properties": {
                "value": {
                  "const": "47"
                }
              }
            },
            "typeCode": {
              "properties": {
                "value": {
                  "const": "636"
                }
              }
            }
          }
        }
      }
    }
  ]
}
