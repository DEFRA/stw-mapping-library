{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "id": "AdditionalInformationSpsNote.json",
  "type": "object",
  "properties": {
    "content": {
      "type": "array",
      "minItems": 1,
      "items": {
        "$ref": "TextType.json",
        "required": [
          "value"
        ]
      }
    },
    "contentCode": {
      "type": "array",
      "items": {
        "$ref": "CodeType.json"
      }
    },
    "subjectCode": {
      "required": [
        "value"
      ]
    }
  },
  "required": [
    "content",
    "subjectCode"
  ]
}
