{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "id": "AdditionalInformationSpsNote.json",
  "AdditionalInformationSpsNote": {
    "type": "object",
    "properties": {
      "content": {
        "type": "array",
        "minItems": 1,
        "items": {
          "$ref": "TextType.json#/TextType",
          "required": [
            "value"
          ]
        }
      },
      "contentCode": {
        "type": "array",
        "items": {
          "$ref": "CodeType.json#/CodeType"
        }
      },
      "subjectCode": {
        "required": [
          "value"
        ]
      }
    },
    "required": [
      "content",
      "subjectCode"
    ]
  }
}
